{
  "name": "formboost",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "imports": {
    "#app": "./src/app.js",
    "#config/*": "./src/config/*",
    "#constants/*": "./src/constants/*",
    "#database/*": "./src/database/*",
    "#middlewares/*": "./src/middlewares/*",
    "#modules/*": "./src/modules/*",
    "#routes/*": "./src/routes/*",
    "#service/*": "./src/service/*",
    "#utils/*": "./src/utils/*"
  },
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "nodemon --watch src --exec \"eslint . --fix && node esbuild.config.js && node ./src/index.js\"",
    "build": "node esbuild.config.js",
    "migration:run": "npx sequelize-cli db:migrate",
    "migration:undo": "npx sequelize-cli db:migrate:undo",
    "migration:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "migration:create": "npx sequelize-cli migration:generate --name",
    "seeder:create": "npx sequelize-cli seed:generate --name",
    "seeder:run": "npx sequelize-cli db:seed --seed",
    "seeder:run:all": "npx sequelize-cli db:seed:all",
    "lint": "eslint .",
    "format": "prettier --write .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@google/genai": "^1.8.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "firebase-admin": "^12.6.0",
    "helmet": "^7.1.0",
    "http-errors": "^2.0.0",
    "joi": "^17.9.1",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.1",
    "nanoid": "^5.1.5",
    "nodemailer": "^6.9.15",
    "resend": "^6.0.3",
    "sequelize": "^6.21.6",
    "winston": "^3.13.0",
    "winston-loki": "^6.1.3",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "esbuild": "^0.25.4",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "husky": "^8.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3"
  }
}
