NODE_ENV=development
PORT=3000

# Database config - REQUIRED
DB_HOST=localhost
DB_PORT=3306
DB_USERNAME=root
DB_PASSWORD=your_password_here
DB_DATABASE=formboost_db
DB_LOGGING=false

# JWT config - REQUIRED for authentication
JWT_SECRET=your_jwt_secret_here
JWT_EXPIREIN=1d

# Frontend URLs - REQUIRED for CORS
WEB_ADMIN_URL=http://localhost:5173
WEB_APP_URL=http://localhost:3001

# Dev mode auth bypass - REQUIRED for simple setup
ALLOW_INSECURE_DEV_AUTH=true

# Optional services - can be left empty for dev
# FIREBASE_* variables not needed with ALLOW_INSECURE_DEV_AUTH=true
# EMAIL_* variables not set - Email disabled  
# LOKI_* variables not set - Loki logging disabled
# DISCORD_* variables not set - Discord webhooks disabled
# GEMINI_API_KEY not set - AI features disabled
